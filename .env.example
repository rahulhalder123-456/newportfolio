# Genkit API Key
GOOGLE_API_KEY=REPLACE_WITH_YOUR_GOOGLE_AI_KEY

# Resend API Key (for contact form)
RESEND_API_KEY=REPLACE_WITH_YOUR_RESEND_API_KEY
EMAIL_RECIPIENT=your-email@example.com

# Admin Password
NEXT_PUBLIC_ADMIN_PASSWORD=your-secure-password

# Firebase Service Account Credentials
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_CLIENT_EMAIL=your-firebase-client-email
# Note: When you copy the private_key from your Firebase service account JSON,
# you must replace all newline characters (\n) with the literal characters \\n.
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\\n...\\n-----END PRIVATE KEY-----\\n"
